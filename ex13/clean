find . -type f -name "#*#"  -exec ls   {} + ; find . -type f -name "#*"  -exec ls   {} + ; find . -type f -name "*#"  -exec ls   {} + ; find . -type f -name "*~"  -exec ls   {} + ; find . -type f -name "#*#"  -exec rm  {} + ; find . -type f -name "#*"  -exec rm  {} + ; find . -type f -name "*#"  -exec rm  {} + ; find . -type f -name "*~"  -exec rm  {} +
